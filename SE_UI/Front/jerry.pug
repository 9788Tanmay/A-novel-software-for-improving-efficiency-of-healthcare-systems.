doctype html
html
    head 
	title=title
	<link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css">
  	body
  	div(class="container")
  	<center><div class='well'><b>Users Details</b></div></center>
	   table(class='table table-stripped')
		thead
		  tr
		      th Sr_no
		      th Registration_ID
		      th name
		      th Email
		      th sex
		      th Disease 
		      th Age
		each item in items
		  tr
		      td= item.Sr_no
		      td= item.Registration_ID
		      td= item.name
		      td= item.Email
		      td= item.sex
		      td= item.Disease 
		      td= item.AgeS